{
  "private": true,
  "name": "@void-aurora/markdown",
  "description": "Markdown renderer based on markdown-it and highlight.js.",
  "license": "MIT",
  "author": "duduluu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/void-aurora/toolkit.git"
  },
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "clean:dist": "rimraf ./dist",
    "prepare": "yarn clean:dist && yarn build",
    "test": "jest",
    "ts": "ts-node -r tsconfig-paths/register -P ../../tsconfig.scripts.json"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@types/highlight.js": "^9.12.3",
    "@types/linkify-it": "^2.1.0",
    "@types/markdown-it": "^0.0.9",
    "@types/markdown-it-anchor": "^4.0.3",
    "@types/markdown-it-container": "^2.0.2",
    "highlight.js": "^9.18.1",
    "highlightjs-vue": "^1.0.0",
    "linkify-it": "^2.2.0",
    "markdown-it": "^10.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0"
  },
  "keywords": [
    "highlight",
    "markdown",
    "markdown-it"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
