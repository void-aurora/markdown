{
  "private": true,
  "name": "@void-aurora/markdown-plugin-highlight",
  "description": "Markdown code highlight plugin.",
  "license": "MIT",
  "author": "duduluu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/void-aurora/toolkit.git"
  },
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "clean:dist": "rimraf ./dist",
    "prepare": "yarn clean:dist && yarn build",
    "test": "jest",
    "ts": "ts-node -r tsconfig-paths/register -P ../../tsconfig.scripts.json"
  },
  "sideEffects": true,
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@types/prismjs": "^1.16.0",
    "@void-aurora/markdown": "^0.0.0",
    "prismjs": "^1.19.0"
  },
  "keywords": [
    "highlight",
    "markdown",
    "markdown-it",
    "prism",
    "void-aurora"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
